{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/acg",
        "destination": "/html/accioncatolica.html"
      },{
        "source": "/testimonios",
        "destination": "/html/testimonio.html"
      },{
        "source": "/concilio",
        "destination": "/html/concilio.html"
      }
    ]
  }
}
